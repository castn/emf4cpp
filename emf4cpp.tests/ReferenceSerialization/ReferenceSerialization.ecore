<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    name="ReferenceSerialization"
    nsURI="http://www.example.org/ReferenceSerialization"
    nsPrefix="ReferenceSerialization">
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Container">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="contents"
        upperBound="-1"
        eType="#//Content"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="references"
        upperBound="-1"
        eType="#//Content"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="child"
        eType="#//Child"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="childRef"
        eType="#//Child"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Content">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="name"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Child"/>
</ecore:EPackage>
